Algoritmo "Exercicios da aula 06 conversor de moeda"

// Segundo Exercicios conversor de moedas

Var
   moeda_brasileira, moeda_americana : REAL

Inicio


   ESCREVA("Digite o valor da moeda brasileira ")
   LEIA(moeda_brasileira)
   moeda_americana <- moeda_brasileira/5.07
   ESCREVA("O valor da moeda brasileira convertida para o valor da moeda americana foi ", moeda_americana)
   
Fimalgoritmo